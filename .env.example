# Supabase Configuration
# 复制此文件为 .env 并填入你的 Supabase 项目信息

# Supabase 项目 URL
SUPABASE_URL=your_supabase_project_url

# Supabase 匿名密钥 (anon key)
SUPABASE_KEY=your_supabase_anon_key

# Giscus 评论系统配置
PUBLIC_GISCUS_HOST=https://giscus.app
PUBLIC_GISCUS_REPO=your_github_username/your_repo_name
PUBLIC_GISCUS_REPO_ID=your_giscus_repo_id
PUBLIC_GISCUS_CATEGORY=Announcements
PUBLIC_GISCUS_CATEGORY_ID=your_giscus_category_id
PUBLIC_GISCUS_LANG=zh-CN

# 其他环境变量
PUBLIC_GOOGLE_SITE_VERIFICATION=your_google_site_verification_code